Cart = (obj) => {\r\n    dispatch(addPizzaToCart(obj));\r\n  }\r\n\r\n    return (\r\n\r\n        <div className=\"container\">\r\n          <div className=\"content__top\">\r\n            <Categories\r\n              activeCategory={category}\r\n              items={categoriesNames}\r\n              onClickCategory={onSelectCategory}\r\n            />\r\n        \r\n            <SortPopup\r\n              onClickSortType={onSelectSortType}\r\n              activeSortType={sortBy.type}\r\n              items={sortItems} \r\n            />\r\n\r\n          </div>\r\n          <h2 className=\"content__title\">Все пиццы</h2>\r\n          <div className=\"content__items\">\r\n            { isLoaded \r\n            ? items&&items.map((obj) =>  \r\n                <PizzaBlock \r\n                  onClickAddPizza={handleAddPizzaToCart}\r\n                  key={obj.id} \r\n                  addedCount={cartItems[obj.id] && cartItems[obj.id].items.length}\r\n                  {...obj} \r\n                  \r\n                />) \r\n            : Array(12).fill(0).map((_, index) => <LoadingBlock key={index}/>)\r\n            }\r\n          </div>\r\n        </div>\r\n        \r\n    );\r\n}\r\n\r\nexport default Home;","C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\pages\\Cart.jsx",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\redux\\store.js",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\redux\\reducers\\filters.js",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\redux\\reducers\\pizzas.js",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\redux\\reducers\\index.js",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\redux\\actions\\pizzas.js",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\src\\redux\\actions\\filters.js",[],"C:\\Users\\Yegor Korenkov\\Desktop\\html+css\\react\\react-pizza\\sr